<mvc:View controllerName="workshop.NavigationStart.controller.Main" xmlns:mvc="sap.ui.core.mvc" displayBlock="true" xmlns="sap.m">
	<Shell id="shell">
		<App id="app">
			<pages>
				<Page id="page" title="{i18n>title}">
					<content></content>
                    <footer>
                        <Bar>
                            <contentRight>
                                <Button text="Navigate" press="handleNavigation" />    
                            </contentRight>
                        </Bar>
                    </footer>
				</Page>
			</pages>
		</App>
	</Shell>
</mvc:View>
